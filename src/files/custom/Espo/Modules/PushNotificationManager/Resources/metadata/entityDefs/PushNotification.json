{
    "fields": {
        "number": {
            "type": "autoincrement",
            "index": true,
            "dbType": "bigint"
        },
        "provider": {
            "type": "varchar"
        },
        "pushNotificationTemplate": {
            "type": "link"
        },
        "templateData": {
            "type": "jsonObject"
        },
        "data": {
            "type": "jsonObject"
        },
        "user": {
            "type": "link"
        },
        "parent": {
            "type": "linkParent"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "sentAt": {
            "type": "datetime",
            "readOnly": true
        },
        "status": {
            "type": "enum",
            "required": true,
            "options": [
                "Created",
                "Sent",
                "Failed"
            ],
            "style": {
                "Created": "default",
                "Sent": "success",
                "Failed": "danger"
            },
            "default": "Created"
        },
        "failReason": {
            "type": "enum",
            "options": [
                "",
                "InvalidState",
                "ProviderNotFound",
                "UserNotFound",
                "ProviderNotResolved",
                "UserNotEligible",
                "SendError",
                "UnexpectedError"
            ],
            "readOnly": true
        }
    },
    "links": {
        "pushNotificationTemplate": {
            "type": "belongsTo",
            "entity": "PushNotificationTemplate"
        },
        "user": {
            "type": "belongsTo",
            "entity": "User"
        },
        "parent": {
            "type": "belongsToParent"
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": true
    }
}
